    <!--集群id-->
    <add key="OLClusterId" value="LibingCluster" />
    <!--服务的id-->
    <add key="OLServiceId" value="LibingService" />
    <!--环境-->
    <add key="OLEnvironment" value="Production" />
    <!--Trace = 0, Debug = 1, Information = 2,  Warning = 3, Error = 4, Critical = 5,  None = 6-->
